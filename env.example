# IBKR MCP Server Environment Configuration
# Copy this file to .env and customize for your setup

# IBKR Connection Settings
IBKR_HOST=127.0.0.1
# 7497 for paper trading, 7496 for live trading
IBKR_PORT=7497
# Unique client ID for your connection
IBKR_CLIENT_ID=1
IBKR_TIMEOUT=10.0

# Account Settings
# Optional: specific account ID (leave empty for auto-detection)
IBKR_ACCOUNT=
# true for paper trading, false for live trading
IBKR_IS_PAPER=true

# Logging
LOG_LEVEL=INFO 