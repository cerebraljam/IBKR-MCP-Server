# IBKR MCP Server Environment Configuration
# Copy this file to .env and customize for your setup

# IBKR Connection Settings
IBKR_HOST=127.0.0.1
IBKR_PORT=7497              # 7497 for paper trading, 7496 for live trading
IBKR_CLIENT_ID=1            # Unique client ID for your connection
IBKR_TIMEOUT=10.0

# Account Settings
IBKR_ACCOUNT=               # Optional: specific account ID (auto-detected if empty)
IBKR_IS_PAPER=true          # true for paper trading, false for live trading

# Logging
LOG_LEVEL=INFO 